{
  "name": "react-native-highlight-overlay",
  "version": "1.5.0",
  "description": "A tinted overlay that allows one or more elements to be highlighted (non-tinted)",
  "main": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-highlight-overlay.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "@typescript-eslint/typescript-estree": "^5.24.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-native-wcandillon": "^3.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-tsc": "^2.0.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-reanimated": "^2.6.0",
    "react-native-jsi-view-helpers": "sergeymild/react-native-jsi-view-helpers#0.71.1",
    "release-it": "^14.2.2",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "^2.6.0",
    "react-native-jsi-view-helpers": "sergeymild/react-native-jsi-view-helpers#0.71.1"
  },
  "dependencies": {
    "react-native-hole-view": "^3.0.0-alpha3"
  }
}
